<table id="phishing-summary-table" class="sortable list">
	<thead>
		<tr>
			<th>Name</th>
			<th>IP Address</th>
			<th>Creation Date</th>
		</tr>
	</thead>
	<tbody>
	<% summary.names_per_result.each do |result, name| %>
		<tr class="<%=h row_cycle %>">
      <td><%= name %></td>
      <td><%= result.address %></td>
			<td><%= "#{time_ago_in_words(result.created_at)} ago" %></td>
		</tr>
	<% end %>
	</tbody>
</table>
