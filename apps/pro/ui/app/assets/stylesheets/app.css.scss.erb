// breadcrumbs styles
#crumbHolder {
  $start-grad: #fcfcfc;
  $end-grad: #f0f0f0;
  padding-top: 11px;
  padding-bottom: 9px;
  #breadcrumb {
    font-size: 0;
    &>a, &>span.last>a {
      font-size: 11px;
      display: inline-block;
      line-height: 21px;
      height: 21px;
      padding: 0 24px 0 12px;
      position: relative;
      background-size: contain;
      &:before {
        position: absolute;
        left: -11px;
        content: '';
        width: 11px;
        height: 21px;
        background-image: image-url('breadcrumbs/deselected_arrow.png');
        background-size: contain;
      }
    }
    &>a {
      color: #555;
      background-image: image-url('breadcrumbs/deselected.png');
      &:first-child {
        border-left: 1px solid #ccc;
        &:before {
          content: none;
          display: none;
        }
      }
    }
    &>span.last {
      &>a {
        color: #fff;
        text-shadow: 0 1px 2px rgba(0,0,0,.3);
        background-image: image-url('breadcrumbs/selected.png');
        padding-right: 12px;

        &:after {
          position: absolute;
          right: -10px;
          content: '';
          width: 11px;
          height: 21px;
          background-image: image-url('breadcrumbs/selected_arrow.png');
          background-size: contain;
        }
        &:before {
          background-image: image-url('breadcrumbs/last_deselected_arrow.png');
        }
      }
    }
  }
}

