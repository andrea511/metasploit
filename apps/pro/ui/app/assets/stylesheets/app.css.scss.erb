$tab-border-color: #ccc;
ul.tabs {
  margin: 8px 0 0 0;
  padding: 0;
}

div.tab_panel {
  background-color: white;
  border-top: 1px solid $tab-border-color;
  padding-top: 10px;
}

ul.tabs li.tab {
  position: relative;
  top: 1px;
  margin-right: 4px;
  display: inline;
  padding: none;
  margin: none;
  list-style: none;
  padding: 0;
  a {
    padding: 8px 18px 8px 30px;
    @include border-radius-separate(4px, 4px, 0, 0);
    border: 1px solid #fff;
    border-bottom: none;
    display: inline-block;
    text-decoration: none;
    color: #666;
    &.active {
      border: 1px solid $tab-border-color;
      background-color: white;
      border-bottom: 1px solid white;
      color: #333;
    }
    &:hover {
      background-color: #fafafa;
    }
    &.active:hover {
      background-color: white;
    }
    &.loot {
      background-position: 2px -193px;
    }
    &.history {
      background-position: 2px -1034px;
    }
    &.post {
      background-position: 2px 6px;
    }
  }
}

ul.tabs li.tab a, #workspace_nav ul.nav_tabs li ul.sub-menu li a, ul.sub-sub-menu li a {
    background-image: image-url('icons/linkBtn.png');
  background-color: none;
  background-repeat: no-repeat;
  $left: 8px;
  $top: 9px;
  &.hosts {
    background-image: image-url('icons/silky/server.png');
    background-position: $left $top;
  }

  &.loots {
    background-image: image-url('icons/silky/briefcase.png');
    background-position: $left $top;
  }

  &.notes {
    background-image: image-url('icons/silky/note.png');
    background-position: $left $top;
  }

  &.vulnerabilities {
    background-position: 4px -153px;
  }

  &.web-vulnerabilities {
    background-position: 4px -153px;
  }

  &.modules {
    background-image: image-url('metasploit_icon.png');
    background-position: $left $top;
  }

  &.map {
    background-image: image-url('icons/silky/map.png');
    background-position: $left $top;
  }
  &.services {
    background-position: $left - 4px $top - 2px;
  }
}
