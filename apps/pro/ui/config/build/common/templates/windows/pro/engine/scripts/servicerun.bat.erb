@echo off
rem START or STOP prosvc
rem --------------------------------------------------------
rem Check if argument is STOP or START

if not ""%1"" == ""START"" goto stop

set /p swap=<@@BITROCK_INSTALLDIR@@\apps\pro\engine\tmp\swap_config
if "%swap%" == "1" "@@BITROCK_INSTALLDIR@@\ruby\bin\ruby" "@@BITROCK_INSTALLDIR@@\apps\pro\ui\script\swap.rb"

net start @@BITROCK_PROSVC_SERVICE_NAME@@
goto end

:stop
net stop @@BITROCK_PROSVC_SERVICE_NAME@@

:end
exit

